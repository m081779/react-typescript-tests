{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "ordered-inputs": [true,{
      "import-sources-order": "any",
      "named-imports-order": "any",
      "grouped-imports": false
    }]
  } 
}
